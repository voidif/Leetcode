//Recursive method, cost too much time
class Solution {
    public int numTrees(int n) {
        return helper(0, n - 1);
    }
    
    public int helper(int begin, int end){
        // if(begin > end){
        //     return 0;
        // }
        if(begin >= end){
            return 1;
        }
        int res = 0;
        for(int i = begin; i <= end; i ++){
            int left = helper(begin, i - 1);
            int right = helper(i + 1, end);
            res = res + left * right;
        }
        return res;
    }
}
