public void getNext(int[][] res, int[] cur, int n){
    if(cur[1] < n - 1 && res[cur[0]][cur[1] + 1] != 0){
        cur[1] ++;
        return;
    }
    if(cur[0] < n - 1 && res[cur[0] + 1][cur[1]] != 0){
        cur[0] ++;
        return;
    }
    if(cur[1] > 0 && res[cur[0]][cur[1] - 1] != 0){
        cur[1] --;
        return;
    }
    if(cur[0] > 0 && res[cur[0] - 1][cur[1]] != 0){
        cur[0] --;
        return;
    }
}
