class Solution {
    public int lengthOfLIS(int[] nums) {
        int[] table = new int[nums.length];
        Arrays.fill(table, 1);
        for(int i = 1; i < nums.length; i ++){
            for(int j = 0;j < i; j ++){
                if(nums[j] < nums[i]){
                    table[i] = Math.max(table[i], table[j] + 1);
                }
            }
        }
        int res = 0;
        for(int tmp : table){
            res = Math.max(res, tmp);
        }
        return res;
    }
}
