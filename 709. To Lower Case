class Solution {
    public String toLowerCase(String str) {
        char[] data = str.toCharArray();
        for(int i = 0; i < data.length; i ++){
            if(data[i] <= 90 && data[i] >= 65){
                data[i] = (char)(data[i] + 32);
            }
        }
        return new String(data);
    }
}
